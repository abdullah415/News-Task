
{{>header}}

<div class="card m-5 py-2 ">
    {{#each currentNews}}
    <div class=" card-body">
    <p class="d-block">
        {{this.title}}
    </p>
    <img src={{this.urlToImage}} width="400" height="250">
    </div>
    <div class="mx-4">
            <a href={{this.url}} class="btn btn-primary mt-4 text-white">read more</a>
        </div>
    {{/each}}
</div>
    

{{>footer}}
